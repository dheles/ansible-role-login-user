Host {{ hostvars[item]['inventory_hostname'] }} {{ project }}-{{ environ }}
	Hostname {{ hostvars[item]['inventory_hostname'] }}
	User {{ login_user }}
	IdentityFile {{ project_pki_subdirectory }}/{{ login_user_key }}
	PreferredAuthentications publickey
	IdentitiesOnly yes
{% if environ == 'dev'  %}
	StrictHostKeyChecking no
	UserKnownHostsFile=/dev/null
{% endif %}
