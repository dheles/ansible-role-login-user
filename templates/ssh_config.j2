Host {{ ansible_fqdn }} {{ ansible_hostname }}
	Hostname {{ ansible_fqdn }}
	User {{ login_user }}
	IdentityFile {{ project_pki_subdirectory }}/{{ login_user_key }}
	PreferredAuthentications publickey
	IdentitiesOnly yes
{% if environ == 'dev'  %}
	StrictHostKeyChecking no
	UserKnownHostsFile=/dev/null
{% endif %}
